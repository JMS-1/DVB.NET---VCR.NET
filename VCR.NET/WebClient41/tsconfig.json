{
  "compileOnSave": false,
  "compilerOptions": {
    "outFile": "scripts/vcrnet.js",
    "removeComments": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "target": "es2015",
    "jsx": "react",
    "module": "amd"
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "Apps"
  ],
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts"
  ]
}
